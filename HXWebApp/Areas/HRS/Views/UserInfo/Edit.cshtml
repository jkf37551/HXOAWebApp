@model DLL.Models.HRSDB.HR_UserModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>编辑</title>
    @Styles.Render("~/Content/bootstrap")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
</head>
<body>
    @using (Html.BeginForm("Edit", "UserInfo", FormMethod.Post, new { @class = "definewidth m20" }))
    {
        @Html.AntiForgeryToken()
        <h4>编辑</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.ID)

        <table class="table table-bordered table-hover definewidth m10">
            <tr>
                <td width="10%" class="tableleft">@Html.LabelFor(model => model.UserId)</td>
                <td>@Html.Encode(Model.UserId)@Html.HiddenFor(model=>Model.UserId)</td>
            </tr>
            <tr>
                <td width="10%" class="tableleft">@Html.LabelFor(model => model.UserName)</td>
                <td>@Html.Encode(Model.UserName)@Html.HiddenFor(model => Model.UserName)</td>
            </tr>
            <tr>
                <td width="10%" class="tableleft">@Html.LabelFor(model => model.STAT2)</td>
                <td>@Html.Encode(Model.STAT2)@Html.HiddenFor(model => Model.STAT2)</td>
            </tr>
            <tr>
                <td width="10%" class="tableleft">@Html.LabelFor(model => model.PERSK)</td>
                <td>@Html.Encode(Model.PERSK)@Html.HiddenFor(model => Model.PERSK)</td>
            </tr>
            <tr>
                <td width="10%" class="tableleft">@Html.LabelFor(model => model.KOSTL)</td>
                <td>@Html.EditorFor(model => Model.KOSTL, new { htmlAttributes = new { @class = "form-control" } })</td>
            </tr>
            <tr>
                <td width="10%" class="tableleft">@Html.LabelFor(model => model.PostPriv)</td>
                <td>@Html.Encode(Model.PostPriv)@Html.HiddenFor(model => Model.PostPriv)</td>
            </tr>
            <tr>
                <td width="10%" class="tableleft">@Html.LabelFor(model => model.UserPosId)</td>
                <td>@Html.Encode(Model.UserPosId)@Html.HiddenFor(model => Model.UserPosId)</td>
            </tr>
            <tr>
                <td width="10%" class="tableleft">@Html.LabelFor(model => model.Sex)</td>
                <td>@Html.Encode(Model.Sex)@Html.HiddenFor(model => Model.Sex)</td>
            </tr>
            <tr>
                <td width="10%" class="tableleft">@Html.LabelFor(model => model.DeptId)</td>
                <td>@Html.Encode(Model.DeptId)@Html.HiddenFor(model => Model.DeptId)</td>
            </tr>
            <tr>
                <td width="10%" class="tableleft">@Html.LabelFor(model => model.DeptId2)</td>
                <td>@Html.Encode(Model.DeptId2)@Html.HiddenFor(model => Model.DeptId2)</td>
            </tr>
            <tr>
                <td width="10%" class="tableleft">@Html.LabelFor(model => model.BUKRS)</td>
                <td>@Html.Encode(Model.BUKRS)@Html.HiddenFor(model => Model.BUKRS)</td>
            </tr>
            <tr>
                <td width="10%" class="tableleft">@Html.LabelFor(model => model.ICNUM)</td>
                <td>@Html.Encode(Model.ICNUM)@Html.HiddenFor(model => Model.ICNUM)</td>
            </tr>
            <tr>
                <td width="10%" class="tableleft">@Html.LabelFor(model => model.PERSG)</td>
                <td>@Html.Encode(Model.PERSG)@Html.HiddenFor(model => Model.PERSG)</td>
            </tr>
            <tr>
                <td width="10%" class="tableleft">@Html.LabelFor(model => model.WERKS)</td>
                <td>@Html.Encode(Model.WERKS)@Html.HiddenFor(model => Model.WERKS)</td>
            </tr>
            <tr>
                <td width="10%" class="tableleft">@Html.LabelFor(model => model.BTRTL)</td>
                <td>@Html.Encode(Model.BTRTL)@Html.HiddenFor(model => Model.BTRTL)</td>
            </tr>
            <tr>
                <td width="10%" class="tableleft">@Html.LabelFor(model => model.ABKRS)</td>
                <td>@Html.Encode(Model.ABKRS)@Html.HiddenFor(model => Model.ABKRS)</td>
            </tr>
            <tr>
                <td width="10%" class="tableleft">@Html.LabelFor(model => model.Mobile)</td>
                <td>@Html.Encode(Model.Mobile)@Html.HiddenFor(model => Model.Mobile)</td>
            </tr>
            <tr>
                <td class="tableleft"></td>
                <td>
                    <input type="submit" class="btn btn-primary" value="保存" /> &nbsp;&nbsp;@Html.ActionLink("取消", "List", "UserInfo", null, new { @class = "btn btn-info", onclick = "javascript:window.history.go(-1); return false;" })
                </td>
            </tr>
        </table>
    }
</body>
</html>
